programa {
  funcao inicio() {
    
    // Variaveis
    
    inteiro valor
    
    // Dados de Usuario

  escreva( "Escreva um valor: ")
  leia(valor)
    
    // Operação
   
    se ( valor > 10 ) {
      escreva("Maior que 10!")
    }
senao se (valor == 10)
{
  escreva(" Valor é igual a 10!")
}
    
    senao {
      escreva(" Não é maior que 10!")
    }

  // Resultado

    escreva ("\n === Fim do Programa ===")
  }
}
